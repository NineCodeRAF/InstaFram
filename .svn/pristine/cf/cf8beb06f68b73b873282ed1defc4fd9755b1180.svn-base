package listeners.dialogListeners;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import view.dialogs.ModulOrParameterDialog;
import view.dialogs.NewModulDialog;

public class OpenModulDialogListener implements ActionListener{

	@Override
	public void actionPerformed(ActionEvent e) {
		// TODO Auto-generated method stub
		ModulOrParameterDialog.getInstance().dispose();
		NewModulDialog.getInstance().setName("");
		NewModulDialog.getInstance().setVisible(true);
	}

}
