package listeners.dialogListeners;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import view.dialogs.NewCompanyDialog;
import view.dialogs.NewProjectDialog;

public class CompanyCancelListener implements ActionListener{
private NewCompanyDialog dialog;
	
	public CompanyCancelListener(NewCompanyDialog dialog) {
		this.dialog = dialog;
	}
	
	@Override
	public void actionPerformed(ActionEvent e) {
		dialog.dispose();
	}
}
